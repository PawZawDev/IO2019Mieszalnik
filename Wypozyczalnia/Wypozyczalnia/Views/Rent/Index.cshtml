@model IEnumerable<Wypozyczalnia.Models.Samochod>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Marka)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Model)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LimitKilometrow)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NrRejestracyjny)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KoniecUmowy)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Marka)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Model)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LimitKilometrow)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NrRejestracyjny)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KoniecUmowy)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
