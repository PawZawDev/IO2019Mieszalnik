@model Wypozyczalnia.Models.Samochod

@{
    ViewBag.Title = "Zakonczenie wynajmu";
}

<h2>Zakonczenie wynajmu</h2>

<h3>Jestes pewny ze chcesz zakonczyc wynajem?</h3>
<div>
    <hr />

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Zakoncz Wynajem" class="btn btn-danger" /> 
            @Html.ActionLink("Wroc", "Index", null , new { @class = "btn btn-info" })
        </div>
    }
</div>
